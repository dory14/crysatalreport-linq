@model List<crReport.Areas.User.Models.UserModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div><a href="@Url.Action("ExportCustomers")"> Report PDF </a></div>
<table class="table">
    <tr>
        <th>
            Username
        </th>
        <th>
            Full Name
        </th>
        <th>
            Password
        </th>
        <th>
            Email
        </th>
        <th>
            Role
        </th>
        <th>
            Created By
        </th>
        <th>
            Created At
        </th>
        <th>
            Updated By
        </th>
        <th>
            Update At
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @item.username
    </td>
    <td>
        @item.full_name
    </td>
    <td>
        @item.password
    </td>
    <td>
        @item.email
    </td>
    <td>
        @item.role
    </td>
    <td>
        @item.created_by
    </td>
    <td>
        @item.created_at
    </td>
    <td>
        @item.updated_by
    </td>
    <td>
        @item.updated_at
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
        @Html.ActionLink("Details", "Details", new { id = item.id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.id })
    </td>
</tr>
    }

</table>
